<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/selectableItemBackground"
    android:maxHeight="@dimen/two_line_min_height">

    <TextView
        android:id="@+id/list_item_four_cell_primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_normal"
        android:layout_marginTop="@dimen/margin_normal"
        android:maxLines="1"
        android:textAppearance="?attr/textAppearanceSubtitle1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="餐饮美食:饮食服务" />

    <TextView
        android:id="@+id/list_item_four_cell_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_normal"
        android:maxLines="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/list_item_four_cell_primary"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="12.80" />

    <TextView
        android:id="@+id/list_item_four_cell_secondary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_normal"
        android:maxLines="1"
        app:layout_constraintEnd_toStartOf="@+id/list_item_four_cell_bottom"
        app:layout_constraintStart_toStartOf="@+id/list_item_four_cell_primary"
        app:layout_constraintTop_toBottomOf="@+id/list_item_four_cell_primary"
        tools:text="日常 堂食-猪杂汤粉" />

    <TextView
        android:id="@+id/list_item_four_cell_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/list_item_four_cell_secondary"
        app:layout_constraintEnd_toEndOf="@+id/list_item_four_cell_top"
        tools:text="蚂蚁花呗" />
</androidx.constraintlayout.widget.ConstraintLayout>